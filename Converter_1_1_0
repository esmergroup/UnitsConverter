						tfAusgabe.setText(String.valueOf(ergebnis1));
					}
					catch(Exception dfse) {
						JOptionPane.showMessageDialog(null, "Sie haben keine GÃ¼ltige Nummer eingegeben!!!");
					}
				}
				else if(eingabe.equals("KilometerProLiter") && ausgabe.equals("LiterPro100Killometer")) {
					try {
						double ergebnis1 = 100 * textFeldEingabe;
						tfAusgabe.setText(String.valueOf(ergebnis1));
					}
					catch(Exception dfse) {
						JOptionPane.showMessageDialog(null, "Sie haben keine GÃ¼ltige Nummer eingegeben!!!");
					}
				}
				else if(eingabe.equals("LiterPro100Killometer") && ausgabe.equals("MeilenProGallone")) {
					try {
						double ergebnis1 = 235.215 * textFeldEingabe;
						tfAusgabe.setText(String.valueOf(ergebnis1));
					}
					catch(Exception dfre) {
						JOptionPane.showMessageDialog(null, "Sie haben keine GÃ¼ltige Nummer eingegeben!!!");
					}
				}
				else if(eingabe.equals("LiterPro100Killometer") && ausgabe.equals("KilometerProLiter")) {
					try {
						double ergebnis1 = 100 * textFeldEingabe;
						tfAusgabe.setText(String.valueOf(ergebnis1));
					}
					catch(Exception dfre) {
						JOptionPane.showMessageDialog(null, "Sie haben keine GÃ¼ltige Nummer eingegeben!!!");
					}
				}
			}
		});
		btnBerechnen.setBounds(11, 113, 180, 25);
		contentPane.add(btnBerechnen);
		
